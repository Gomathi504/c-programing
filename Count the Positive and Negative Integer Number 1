#include <stdio.h>

int main() {
    int n, value;
    int positive = 0, negative = 0;

    // Read number of elements
    scanf("%d", &n);

    // Loop through each element
    for(int i = 0; i < n; i++) {
        scanf("%d", &value);
        if(value > 0)
            positive++;
        else if(value < 0)
            negative++;
    }

    // Print the results with 2 decimal places
    printf("Count of Positive Integer is %.2f\n", (float)positive);
    printf("Count of Negative Integer is %.2f\n", (float)negative);

    return 0;
}
